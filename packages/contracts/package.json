{
  "name": "@cash-io/contracts",
  "version": "1.0.0",
  "description": "Smart contracts for Cash.io protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "dev": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:subnet": "hardhat run scripts/deploy.ts --network avalancheSubnet",
    "clean": "hardhat clean && rm -rf dist typechain-types",
    "lint": "solhint 'contracts/**/*.sol'"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "hardhat": "^2.19.4",
    "solhint": "^4.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^17.2.4",
    "snarkjs": "^0.7.3"
  }
}
