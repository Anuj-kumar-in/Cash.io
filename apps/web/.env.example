# ============ Solana Bridge Contracts ============

# Solana uses Wormhole for bridging
VITE_SOLANA_WORMHOLE_ADDRESS=
VITE_SOLANA_DEVNET_WORMHOLE_ADDRESS=

# ============ Feature Flags ============

# Enable testnets in UI
VITE_ENABLE_TESTNETS=true

# Enable Solana bridging
VITE_ENABLE_SOLANA=true

# Enable Bitcoin L2s
VITE_ENABLE_BITCOIN_L2S=true

# ============ IPFS Configuration (Decentralized Storage) ============

# IPFS Gateway URL for reading content from IPFS
# Options: https://gateway.pinata.cloud, https://ipfs.io, https://cloudflare-ipfs.com
VITE_IPFS_GATEWAY_URL=https://gateway.pinata.cloud

# IPFS API URL for uploading content
VITE_IPFS_API_URL=https://api.pinata.cloud

# Pinata JWT token for authenticated uploads
# Get your JWT at: https://app.pinata.cloud/developers/api-keys
VITE_IPFS_JWT=

# Alternative: Infura IPFS (uncomment and use instead of Pinata)
# VITE_IPFS_API_KEY=your-infura-project-id
# VITE_IPFS_API_SECRET=your-infura-project-secret
