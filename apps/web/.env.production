# ============ Solana Bridge (via Wormhole) ============

# Wormhole Token Bridge on Solana (Mainnet)
VITE_SOLANA_WORMHOLE_ADDRESS=wormDTUJ6AWPNvk59vGQbDvGJmqbDTdgWgAqcLBCgUb

# ============ Feature Flags (Production) ============

# Enable testnet chains in UI (set to false for production)
VITE_ENABLE_TESTNETS=false

# Enable Solana bridging (set to true when ready)
VITE_ENABLE_SOLANA=true

# Enable Bitcoin L2s
VITE_ENABLE_BITCOIN_L2S=true

# Analytics & Monitoring
VITE_ENABLE_ANALYTICS=true
VITE_SENTRY_DSN=https://your-sentry-dsn
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# ============ IPFS Configuration (Decentralized Storage) ============

# IPFS Gateway URL for reading content from IPFS
VITE_IPFS_GATEWAY_URL=https://gateway.pinata.cloud

# IPFS API URL for uploading (uses Pinata by default)
VITE_IPFS_API_URL=https://api.pinata.cloud

# Pinata JWT token for authenticated uploads (get at https://app.pinata.cloud/developers/api-keys)
VITE_IPFS_JWT=your-pinata-jwt-token

# ============ Security ============

# Content Security Policy nonce (auto-generated in production builds)
VITE_CSP_NONCE=

# Allowed origins for CORS
VITE_ALLOWED_ORIGINS=https://cash.io,https://app.cash.io
