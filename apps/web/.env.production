# Cash.io Production Environment
# ================================
# Copy this to .env and fill in your values

# ============ Core Configuration ============

# WalletConnect Project ID (REQUIRED - get at https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# ============ Hub Chain (Cash.io Subnet) ============

# Deployed contract addresses on Cash.io Subnet
VITE_SHIELDED_POOL_ADDRESS=0x...
VITE_ZK_VERIFIER_ADDRESS=0x...
VITE_COMMITMENT_TREE_ADDRESS=0x...
VITE_ACCOUNT_FACTORY_ADDRESS=0x...
VITE_PAYMASTER_ADDRESS=0x...
VITE_ENTRY_POINT_ADDRESS=0x5FF137D4b0FDCD49DcA30c7CF57E578a026d2789

# Cash.io Subnet RPC
VITE_HUB_RPC_URL=https://rpc.cash.io

# ============ API Endpoints (Production) ============

VITE_BLOB_STORAGE_URL=https://api.cash.io/blob
VITE_PROVER_URL=https://api.cash.io/prover
VITE_BUNDLER_URL=https://api.cash.io/bundler
VITE_RELAYER_URL=https://api.cash.io/relayer

# ============ EVM Mainnet Bridge Contracts ============

# Ethereum Mainnet (Chain ID: 1)
VITE_ETH_BRIDGE_ADDRESS=0x...

# Polygon (Chain ID: 137)
VITE_POLYGON_BRIDGE_ADDRESS=0x...

# Arbitrum One (Chain ID: 42161)
VITE_ARBITRUM_BRIDGE_ADDRESS=0x...

# Optimism (Chain ID: 10)
VITE_OPTIMISM_BRIDGE_ADDRESS=0x...

# Avalanche C-Chain (Chain ID: 43114)
VITE_AVALANCHE_BRIDGE_ADDRESS=0x...

# Base (Chain ID: 8453)
VITE_BASE_BRIDGE_ADDRESS=0x...

# BNB Smart Chain (Chain ID: 56)
VITE_BSC_BRIDGE_ADDRESS=0x...

# zkSync Era (Chain ID: 324)
VITE_ZKSYNC_BRIDGE_ADDRESS=0x...

# Linea (Chain ID: 59144)
VITE_LINEA_BRIDGE_ADDRESS=0x...

# Scroll (Chain ID: 534352)
VITE_SCROLL_BRIDGE_ADDRESS=0x...

# Blast (Chain ID: 81457)
VITE_BLAST_BRIDGE_ADDRESS=0x...

# Mantle (Chain ID: 5000)
VITE_MANTLE_BRIDGE_ADDRESS=0x...

# ============ Bitcoin L2 / Sidechain Bridges ============

# Rootstock (Chain ID: 30)
VITE_ROOTSTOCK_BRIDGE_ADDRESS=0x...

# BOB - Build on Bitcoin (Chain ID: 60808)
VITE_BOB_BRIDGE_ADDRESS=0x...

# Merlin Chain (Chain ID: 4200)
VITE_MERLIN_BRIDGE_ADDRESS=0x...

# Bitlayer (Chain ID: 200901)
VITE_BITLAYER_BRIDGE_ADDRESS=0x...

# Core (Chain ID: 1116)
VITE_CORE_BRIDGE_ADDRESS=0x...

# ============ Solana Bridge (via Wormhole) ============

# Wormhole Token Bridge on Solana (Mainnet)
VITE_SOLANA_WORMHOLE_ADDRESS=wormDTUJ6AWPNvk59vGQbDvGJmqbDTdgWgAqcLBCgUb

# ============ Feature Flags (Production) ============

# Enable testnet chains in UI (set to false for production)
VITE_ENABLE_TESTNETS=false

# Enable Solana bridging (set to true when ready)
VITE_ENABLE_SOLANA=true

# Enable Bitcoin L2s
VITE_ENABLE_BITCOIN_L2S=true

# Analytics & Monitoring
VITE_ENABLE_ANALYTICS=true
VITE_SENTRY_DSN=https://your-sentry-dsn
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# ============ Security ============

# Content Security Policy nonce (auto-generated in production builds)
VITE_CSP_NONCE=

# Allowed origins for CORS
VITE_ALLOWED_ORIGINS=https://cash.io,https://app.cash.io
