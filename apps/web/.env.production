# Cash.io Production Environment
# ================================
# Copy this to .env and fill in your values

# ============ Core Configuration ============

# WalletConnect Project ID (REQUIRED - get at https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# ============ Hub Chain (Cash.io Subnet) ============

# Deployed contract addresses on Cash.io Subnet
VITE_SHIELDED_POOL_ADDRESS=0x688e2C8dF27C925bdCBa5c7DFafD939A760d0466
VITE_ZK_VERIFIER_ADDRESS=0x176A451EaABfFB08DDe514C2dA9F56BBAd12919B
VITE_COMMITMENT_TREE_ADDRESS=0xedF026f2DA00e4b693b99AdB17bB3Cad01330b25
VITE_ACCOUNT_FACTORY_ADDRESS=0x67e86D82f89199DD1BC2Bff9D2EA9FCF1E58aB92
VITE_PAYMASTER_ADDRESS=0x6B83455076ce7040142eECE310d29ed534D8eD59
VITE_ENTRY_POINT_ADDRESS=0x52B68a06E8De47A2362885D9859b46D7e550EF8a

# Cash.io Subnet RPC
VITE_HUB_RPC_URL=https://rpc.cash.io

# ============ API Endpoints (Production) ============

VITE_BLOB_STORAGE_URL=https://api.cash.io/blob
VITE_PROVER_URL=https://api.cash.io/prover
VITE_BUNDLER_URL=https://api.cash.io/bundler
VITE_RELAYER_URL=https://api.cash.io/relayer

